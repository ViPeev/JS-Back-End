<h1>Browser</h1>
<form action="/" method="post">
  <input type="text" class="search" name="search" placeholder="Search..." />
  <input
    type="number"
    name="from"
    class="difficulty"
    placeholder="Difficulty from..."
    min="1"
    max="6"
  />
  <span>-</span>
  <input
    type="number"
    name="to"
    class="difficulty"
    placeholder="Difficulty to..."
    min="1"
    max="6"
  />
  <input type="submit" value="search" />
</form>
{{#each cubes}}
{{>cube}}
{{/each}}
</div>